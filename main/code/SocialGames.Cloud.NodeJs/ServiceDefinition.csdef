    <Imports>
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
    </Imports>
    <Endpoints>
      <InputEndpoint name="SocketIO" protocol="tcp" port="8080" />
      <InputEndpoint name="NetSocket" protocol="tcp" port="8124" />
      <InputEndpoint name="NodeDebug" protocol="tcp" port="5858" />
    </Endpoints>
    <LocalResources>
    </LocalResources>
  </WorkerRole>
</ServiceDefinition>